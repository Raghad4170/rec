<odoo>
     
    <template id="thankyou_msg_bills" name="تم إنشاء الفاتورة بنجاح">
            <t t-call="website.layout">
     <div class="wrap  d-flex">
                <div class="container  d-flex flex-column mb-5">
                    <div  style="padding-top:20px;">                
                    <p><h1>تم إنشاء الفاتورة بنجاح</h1></p>
                    </div>
                </div>
            </div>  
            </t>
    </template>
    <template id="create_bills" name="إنشاء الفاتورة">
        <t t-call="website.layout">
        <div class="container pt-5">
            <div align="center">
                <h2 class="text-muted">
                    إنشاء الفاتورة 
                </h2>
            </div>
            <div class="pt-5"/>
            <span class="hidden" data-for="create_bills"/>
              <div id="create_bills_section">
                <section id="forms">
                 <div class="container">                                        
                  <form id="contactus_form_cl" action="/submit_bills" method="post" enctype="multipart/form-data" class="o_mark_required" data-mark="*" data-model_name="mail.mail" data-success-mode="redirect" data-success-page="/contactus-thank-you" data-pre-fill="true">
                      <div>
                            <div class="form-group col-12 s_website_form_field s_website_form_model_required" data-type="char">
                                    <div class="row s_col_no_resize s_col_no_bgcolor">
                                        <label class="col-form-label col-auto s_website_form_label" style="width: 200px" for="name">
                                            <span class="s_website_form_label_content">اسم الفاتورة</span>
                                            <span class="s_website_form_mark"> *</span>
                                        </label>
                                        <div class="col-sm col-xs-12">
                                            <input id="name" type='text' class='form-control s_website_form_input' name="name" required="True"/>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group col-12 s_website_form_field s_website_form_model_required" data-type="float">
                                    <div class="row s_col_no_resize s_col_no_bgcolor">
                                        <label class="col-form-label col-auto s_website_form_label" style="width: 200px" for="expense_amount">
                                            <span class="s_website_form_label_content">مبلغ الفاتورة</span>
                                        </label>
                                        <div class="col-sm col-xs-12">
                                            <input id="expense_amount" type='float' class='form-control s_website_form_input' name="expense_amount"/> 
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group col-12 s_website_form_field">
                                    <div class="row s_col_no_resize s_col_no_bgcolor">
                                        <label class="col-form-label col-auto s_website_form_label" style="width: 200px" for="bank">
                                            <span class="s_website_form_label_content">الحوالة البنكية</span>
                                        </label>
                                        <div class="col-sm col-xs-12">
                                            <input id="bank" type="file" name="bank" class="form-control s_website_form_input"/>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group col-12 s_website_form_field">
                                        <div class="row s_col_no_resize s_col_no_bgcolor">
                                            <label class="col-form-label col-auto s_website_form_label" style="width: 200px" for="file">
                                                <span class="s_website_form_label_content">ملف الفاتورة</span>
                                            </label>
                                            <div class="col-sm col-xs-12">
                                                <input id="file" type="file" name="file" class="form-control s_website_form_input"/>
                                            </div>
                                        </div>
                                </div>
                                <div class="form-group col-12 s_website_form_field s_website_form_model_required" data-type="char">
                                    <div class="row s_col_no_resize s_col_no_bgcolor">
                                        <label class="col-form-label col-auto s_website_form_label" style="width: 200px" for="note">
                                            <span class="s_website_form_label_content">ملاحظات</span>
                                        </label>
                                        <div class="col-sm col-xs-12">
                                            <input id="note" type='text' class='form-control s_website_form_input' name="note"/>
                                        </div>
                                    </div>
                                </div>
                                <div align="left">
                                    <button type="submit" id="submit_msg" class="btn btn-primary">إرسال</button>  
                                </div> 
                            </div>
                      </form>
                  </div>
                </section>
             </div> 
           </div>
        </t>
    </template>
    
